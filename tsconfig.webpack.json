{
	"compilerOptions": {
		"moduleResolution": "node",
		"module": "es2015",
		"outDir": "build",
		"sourceMap": true,
		"target": "es2019",
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strictNullChecks": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "react"
	}
}
